{
  "name": "r3f-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@craco/craco": "^5.6.4",
    "@react-spring/three": "^9.0.0-rc.3",
    "babel-plugin-glsl": "^1.0.0",
    "craco-glslify-loader": "^1.0.2",
    "craco-raw-loader": "^1.0.1",
    "cz-conventional-changelog": "^3.2.0",
    "drei": "^0.0.72",
    "glsl-noise": "^0.0.0",
    "postprocessing": "^6.16.0",
    "react": "^17.0.0-rc.0",
    "react-dom": "^17.0.0-rc.0",
    "react-postprocessing": "^1.1.4",
    "react-scripts": "^4.0.0-next.77",
    "react-three-fiber": "5.0.0-beta.11",
    "react-three-gui": "^0.1.5",
    "three": "^0.119.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "eject": "craco eject",
    "cm": "git-cz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "tabWidth": 2
  },
  "lint-staged": {
    "*.(js|ts|tsx)": [
      "eslint --fix",
      "prettier --write",
      "jest  --findRelatedTests --config jestconfig.json",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "commitizen": "^4.1.2",
    "glslify-loader": "^2.0.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "raw-loader": "^4.0.1",
    "lint-staged": "^10.2.11"
  }
}
